   "source": [
    "num = eval(input(\"Enter the number : \"))\n",
    "digit = eval(input(\"Enter the digit : \"))\n",
    "count = 0\n",
    "while num>0:\n",
    "    rem = num%10\n",
    "    if rem == digit:\n",
    "        count+=1\n",
    "    num = num//10\n",
    "print(\"The digit comes \",count,\" times\")"
   ]

   "source": [
    "num = eval(input(\"Enter the number : \"))\n",
    "cnum = num\n",
    "revNum = 0\n",
    "while num>0:\n",
    "    rem = num%10\n",
    "    revNum = revNum *10 +rem\n",
    "    num = num//10\n",
    "print(\"The reverse number is : \",revNum)\n",
    "if cnum<revNum:\n",
    "    print(\"the reverse number is greater than the original number...\")"
   ]

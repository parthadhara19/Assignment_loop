   "source": [
    "num = int(input(\"Enter a number: \"))\n",
    "\n",
    "while num >= 10:\n",
    "    digit_sum = 0\n",
    "    temp = num\n",
    "\n",
    "    while temp > 0:\n",
    "        digit_sum += temp % 10\n",
    "        temp //= 10\n",
    "\n",
    "    num = digit_sum\n",
    "\n",
    "print(\"Single digit:\", num)"
   ]
